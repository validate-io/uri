    "string",
    "isstring"
  ],
  "bugs": {
    "url": "https://github.com/validate-io/uri/issues"
  },
  "dependencies": {
    "validate.io-string": "^1.0.2"
  },
  "devDependencies": {
    "chai": "2.x.x",
    "mocha": "2.x.x",
    "coveralls": "^2.11.1",
